{
  "name": "axentax-mcp-server",
  "version": "0.1.0",
  "description": "MCP server for Axentax DSL to MIDI conversion",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "bundle": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/axentax-mcp.cjs --banner:js='#!/usr/bin/env node'",
    "start:bundle": "node dist/axentax-mcp.cjs"
  },
  "keywords": [
    "axentax",
    "midi",
    "mcp",
    "compiler"
  ],
  "author": "Yasu",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axentax-compiler": "^1.0.1",
    "express": "^4.18.2",
    "pino": "^8.16.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.8.0",
    "esbuild": "^0.23.0",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
